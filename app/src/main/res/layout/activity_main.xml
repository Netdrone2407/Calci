   <TextView
      android:id="@+id/text"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="40sp"
      android:text="@string/change"/>

   <TextView
       android:id="@+id/text1"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_below="@+id/text"
       android:text="@string/change1"
       android:textSize="40sp" />
   <View
       android:layout_width="match_parent"
       android:layout_height="1dp"
       android:layout_marginTop="440dp"
       android:background="@android:color/white" />
   <TextView
       android:id="@+id/btn1"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_above="@+id/btn5"
       android:textColor="#F98500"
      android:text="1"
       android:textSize="50sp"
       android:layout_marginHorizontal="35dp"
       />
      <TextView
          android:id="@+id/btn2"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_toEndOf="@+id/btn1"
          android:layout_above="@+id/btn6"
          android:textSize="50sp"
          android:textColor="#F98500"
          android:layout_marginHorizontal="35dp"
          android:text="2"
          />
      <TextView
          android:id="@+id/btn3"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_toEndOf="@+id/btn2"
          android:layout_above="@+id/btn7"
          android:textSize="50sp"
          android:textColor="#F98500"
          android:layout_marginHorizontal="35dp"
          android:text="3"
          />

   <TextView
       android:id="@+id/btn4"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_toEndOf="@+id/btn3"
       android:layout_above="@+id/btn8"
       android:textSize="50sp"
       android:textColor="#F98500"
       android:layout_marginHorizontal="35dp"
       android:text="C"
       />



   <TextView
       android:id="@+id/btn5"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_above="@+id/btn9"
       android:layout_marginHorizontal="35dp"
       android:textColor="#F98500"
       android:textSize="50sp"
       android:text="4"
       />
   <TextView
       android:id="@+id/btn6"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_toEndOf="@+id/btn5"
       android:layout_above="@+id/btn10"
       android:layout_marginHorizontal="35dp"
       android:textColor="#F98500"
       android:textSize="50sp"
       android:text="5"
       />
   <TextView
       android:id="@+id/btn7"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_toEndOf="@+id/btn6"
       android:layout_above="@+id/btn11"
       android:textSize="50sp"
       android:layout_marginHorizontal="35dp"
       android:textColor="#F98500"
       android:text="6"
       />
   <TextView
       android:id="@+id/btn8"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_toEndOf="@+id/btn7"
       android:layout_above="@+id/btn12"
       android:layout_marginHorizontal="35dp"
       android:textColor="#F98500"
       android:textSize="50sp"
       android:text="+"
       />

      <TextView
          android:id="@+id/btn9"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/btn13"
          android:textColor="#F98500"
          android:textSize="50sp"
          android:layout_marginHorizontal="35dp"
          android:text="7"
          />
      <TextView
          android:id="@+id/btn10"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_toEndOf="@+id/btn9"
          android:layout_above="@+id/btn14"
          android:textColor="#F98500"
          android:layout_marginHorizontal="35dp"
          android:textSize="50sp"
          android:text="8"
          />
      <TextView
          android:id="@+id/btn11"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_toEndOf="@+id/btn10"
          android:textColor="#F98500"
          android:layout_above="@+id/btn15"
          android:layout_marginHorizontal="35dp"
          android:text="9"
          android:textSize="50sp"
          />
      <TextView
          android:id="@+id/btn12"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_toEndOf="@+id/btn11"
          android:layout_above="@+id/btn16"
          android:layout_marginHorizontal="35dp"
          android:textColor="#F98500"
          android:textSize="50sp"
          android:text=" -"
          />


      <TextView
          android:id="@+id/btn13"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_above="@+id/run"
          android:textSize="50sp"
          android:textColor="#F98500"
          android:layout_marginHorizontal="35dp"
          android:text="0"
          />
      <TextView
          android:id="@+id/btn14"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_toEndOf="@+id/btn13"
          android:layout_above="@+id/run"
          android:textColor="#F98500"
          android:layout_marginHorizontal="35dp"
          android:textSize="50sp"
          android:gravity="center_horizontal"
          android:text=" ."
          />
      <TextView
          android:id="@+id/btn15"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_toEndOf="@+id/btn14"
          android:layout_above="@+id/run"
          android:layout_marginHorizontal="35dp"
          android:textColor="#F98500"
          android:textSize="50sp"
          android:text=" /"
          />
      <TextView
          android:id="@+id/btn16"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_toEndOf="@+id/btn15"
          android:layout_above="@+id/run"
          android:layout_marginHorizontal="35dp"
          android:textColor="#F98500"
          android:textSize="50sp"
          android:text="*"
       />


   <TextView
       android:id="@+id/ac"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="40sp"
       android:layout_alignParentBottom="true"
       android:layout_marginHorizontal="35dp"
       android:textColor="#F98500"
       android:text="AC"/>

   <TextView
       android:id="@+id/btn17"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textSize="40sp"
       android:layout_toEndOf="@+id/ac"
       android:textColor="#F98500"
       android:layout_alignParentBottom="true"
       android:layout_marginHorizontal="20dp"
       android:text="%"/>
   <Button
       android:id="@+id/run"
       android:layout_width="200dp"
       android:layout_height="wrap_content"
       android:textColor="#F98500"
       android:gravity="center"
       android:layout_toEndOf="@+id/btn17"
       android:layout_marginHorizontal="35dp"
       android:layout_alignParentBottom="true"
       android:textSize="40sp"
       android:text="="
      />



</RelativeLayout>